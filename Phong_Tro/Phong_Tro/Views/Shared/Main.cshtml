				addWard("p234", "Phường Bình Trị Đông");
				addWard("p235", "Phường Bình Trị Đông A");
				addWard("p236", "Phường Bình Trị Đông B");
				addWard("p237", "Phường Tân Tạo");
				addWard("p238", "Phường Tân Tạo A");
				break;

		}
	}
	function addWard(value, name) {
		var wardSelect = document.getElementById("ward");
		var option = document.createElement("option");
		option.value = value;
		option.textContent = name;
		wardSelect.appendChild(option);
	}

</script>
